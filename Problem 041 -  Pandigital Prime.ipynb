    "from sympy import isprime\n",
    "for p in permutations('7654321',7):\n",
    "    string = \"\"\n",
    "    for i in p:\n",
    "        string += i\n",
    "    if isprime(int(string)):\n",
    "        print(string)\n",
    "        break"
   ]
