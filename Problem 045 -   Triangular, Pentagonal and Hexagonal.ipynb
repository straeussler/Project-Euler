    "n = 144\n",
    "while n > 0:\n",
    "    #hexagonal number (and thus triangular number)\n",
    "    x = n*(2*n-1)\n",
    "    #check if pentagonal\n",
    "    if ((24*x+1)**0.5 +1) % 6 == 0:\n",
    "        print(x)\n",
    "        break\n",
    "    n += 1"
