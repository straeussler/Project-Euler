    "from math import factorial\n",
    "total = 0\n",
    "for i in range(3,6*factorial(9)):\n",
    "    factorial_sum = 0\n",
    "    for x in str(i):\n",
    "        factorial_sum += factorial(int(x))\n",
    "    if factorial_sum == i:\n",
    "        total += i\n",
    "print(total)"
   ]
