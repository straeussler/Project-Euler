    "max_sum = 0\n",
    "for a in range(100):\n",
    "    for b in range(100):\n",
    "        digits = str(a**b)\n",
    "        digit_sum = 0\n",
    "        for digit in digits:\n",
    "            digit_sum += int(digit)\n",
    "        if digit_sum > max_sum:\n",
    "            max_sum = digit_sum\n",
    "print(max_sum)\n",
    "        "
