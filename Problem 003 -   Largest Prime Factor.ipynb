    "def is_prime(n):\n",
    "    if n % 2 == 0:\n",
    "        print(2)\n",
    "        n = n / 2\n",
    "    for i in range(3,int(math.sqrt(n))+1,2):\n",
    "        if n % i == 0:\n",
    "            print(i)\n",
    "            n = n / i\n",
